<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:startDestination="@id/navigation_loading">
    <fragment
        android:id="@+id/navigation_loading"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.LoadingFragment"
        android:label="Loading"/>
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.home.HomeFragment"
        android:label="@string/title_home">
    </fragment>
    <fragment
        android:id="@+id/navigation_scripture"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.scripture.ScriptureViewer"
        android:label="Scripture">
        <argument
            android:name="chapter"
            app:argType="string"/>
        <argument
            android:name="psalms"
            app:argType="boolean"/>
        <argument
            android:name="iteration"
            app:argType="integer"/>
    </fragment>
    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.MainSettings"
        android:label="Settings">
    </fragment>
    <fragment
        android:id="@+id/navigation_information"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.InformationFragment"
        android:label="Information" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.Notifications.NotificationsFragment"
        android:label="Notifications"/>
    <fragment
        android:label="Manual"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.ManualOverrides.ManualListSet"
        android:id="@+id/navigation_manual" />
    <fragment
        android:label="Manual Numerical"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.ManualOverrides.ManualListNumericalSet"
        android:id="@+id/navigation_manual_numerical" />
    <fragment
        android:id="@+id/navigation_stats"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.StatisticsFragment"
        android:label="Statistics">
    </fragment>
    <fragment
        android:id="@+id/navigation_plan_type"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.PlanSettings.PlanTypeFragment"
        android:label="Plan Type Menu"/>
    <fragment
        android:id="@+id/navigation_plan_system"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.PlanSettings.PlanSystemFragment"
        android:label="Plan System Menu"/>
    <fragment
        android:id="@+id/navigation_account_settings"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.Account.AccountSettingsFragment"
        android:label="Account Settings"/>
    <fragment
        android:id="@+id/navigation_update_email"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.Account.UpdateEmailFragment"
        android:label="Update Email">
        <argument
            android:name="error"
            app:argType="string"/>
        <argument
            android:name="email"
            app:argType="string"/>
    </fragment>
    <fragment
        android:id="@+id/navigation_update_password"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.Account.UpdatePasswordFragment"
        android:label="Update Password">
        <argument
            android:name="error"
            app:argType="string"/>
    </fragment>
    <fragment
        android:id="@+id/navigation_confirm_delete"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.Account.DeleteAccountFragment"
        android:label="Delete Account">
        <argument
            android:name="error"
            app:argType="string"/>
    </fragment>
    <fragment
        android:id="@+id/navigation_overrides"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.ManualOverrides.OverridesFragment"
        android:label="Overrides"/>
    <fragment
        android:id="@+id/navigation_plan_settings"
        android:name="com.theunquenchedservant.granthornersbiblereadingsystem.ui.settings.PlanSettings.PlanSettingsFragment"
        android:label="PlanSettings" />




</navigation>